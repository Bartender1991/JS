{
	"name": "youtube notificator",
	"version": "1.0",
	"description": "utilizalo para enterarte de nuevos videos",
	"manifest_version": 2,
	"author": "mixologyzt",
	"icons": {
		"16": "16.png",
		"128": "128.png"
	},
	"browser_action":{
		"default_title": "youtube notificator",
		"default_icon": "16.png",
		"default_popup": "popup.html"
	},
	"background":{
		"page": "background.html"
	},
	"permissions": [
		"notifications",
		"storage",
		"tabs",
		"http://*/*",
		"https://*/*",
		"cookies",
		"https://www.youtube.com/*"
	],
	"web_accessible_resources": [
		"128.png"
	]
}